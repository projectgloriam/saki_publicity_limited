<p id="notice"><%= notice %></p>

<h1>Listing Staffs</h1>

<table>
  <thead>
    <tr>
      <th>Staff first name</th>
      <th>Staff last name</th>
      <th>Staff email</th>
      <th>Staff telephone</th>
      <th>Organization</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @staffs.each do |staff| %>
      <tr>
        <td><%= staff.staff_first_name %></td>
        <td><%= staff.staff_last_name %></td>
        <td><%= staff.staff_email %></td>
        <td><%= staff.staff_telephone %></td>
        <td><%= staff.organization.organization_name %></td>
        <td><%= link_to 'Show', staff %></td>
        <td><%= link_to 'Edit', edit_staff_path(staff) %></td>
        <td><%= link_to 'Destroy', staff, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Staff', new_staff_path %>
